[base]
i2c_device="/dev/i2c-1"
webserver = true
vfs = false
# Valid log levels: panic, error, info, debug
loglevel=debug

[digitalout]
allowed_user = "smartpi,root"

[etemperature]
shared_file_enabled=true
shared_dir="/var/run"
shared_file="smartpi_etemperature_values"
allowed_user = "smartpi,root"
i2c_address = 0x52
# Measurements each minute
samplerate = 6